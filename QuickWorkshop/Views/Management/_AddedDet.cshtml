@model QuickWorkshop.ViewModels.DetailsLists

<div class="ptable">
    <table>
        <tr>
            <th>ID Producto</th>
            <th>Producto</th>
            <th>Cantidad</th>
            <th>Precio</th>
        </tr>
        @foreach(var p in Model.orderpdetails)
        {
        <tr>
            <td>@p.Productid</td>
            <td>@Model.products.Find(x => x.ProductId == p.Productid).Name</td>
            <td>@p.Quantity</td>
            <td>@p.Price</td>
        </tr>
        }       
    </table>
</div>
<div class="stable">
    <table>
        <tr>
            <th>ID Servicio</th>
            <th>Servicio</th>
            <th>Precio</th>
        </tr>
        @foreach (var s in Model.ordersdetails)
        {
        <tr>
            <td>@s.ServiceId</td>
            <td>@Model.services.Find(x=> x.ServiceID == s.ServiceId).Name</td>
            <td>@s.Price</td>
        </tr>
        }        
    </table>
</div>
